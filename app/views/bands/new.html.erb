<h1>Create Band!</h1>


<form action="<%= bands_url %>" method="POST">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">
  
  <label for="band_name">Name</label>
  <input
    type="text"
    name="band[name]"
    id="name"
    value="<%= @band.name %>">
  <br>

  <input type="submit" value="Create Band!">
</form>
